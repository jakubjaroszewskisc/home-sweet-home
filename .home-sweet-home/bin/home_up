#!/bin/bash

ref_root="${HOME_SWEET_HOME}/homedir"
find "${ref_root}" -type f | while read path; do
    rel_path="${path#${ref_root}/}"
    if ! cmp "${ref_root}/${rel_path}" "$HOME/${rel_path}" > /dev/null; then
        echo ${rel_path}
        cp -p "$HOME/${rel_path}" "${ref_root}/${rel_path}"
    fi
done
